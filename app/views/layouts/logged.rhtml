  <title>Sistema de Credito</title>
  <meta name="keywords" content="credito" />
  <meta name="Small Window " content="" />
  <!--estilos del tema por defecto -->
  <%= stylesheet_link_tag "default"%>
  <%= stylesheet_link_tag "miestilo"%>
  <%= javascript_include_tag 'prototype' %>
  <%= javascript_include_tag 'misfunciones' %>
