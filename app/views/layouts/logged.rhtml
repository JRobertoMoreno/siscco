                          <p class="meta">
                            <small>Bienvenido <%=session['usuario']%><br />
                          	Ultimo accesso: <%=Time.now.strftime("%Y-%m-%d")%>
                            </small>
                          </p>
                        <%end%>

                        <!-- contenido -->
			<div class="entry">
              <p style="color: green"><%= flash[:notice] %></p>
