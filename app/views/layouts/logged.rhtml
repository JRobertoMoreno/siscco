<body>
<!-- start header -->
<div id="header">
	<div id="logo">
		<h1><a href="#">Sistema de </a></h1>
		<h2><a href="http://www.freecsstemplates.org/">Créditos</a></h2>
	</div>
	<div id="menu">
		<ul>
			
                        <li class="active"><%=link_to 'catalogos', :controller => "catalogos"%></li>
			<li><%=link_to 'Créditos', :controller => "creditos", :action => "menu"%></li>
			<li><%=link_to 'Información', :controller => "informacion", :action => "busca_credito"%></li>
			<li><%=link_to 'Administracion', :controller => "administracion", :action => "index"%></li>

                        <!-- verificamos si esta loggeado -->

                         <% if session['user']%>
                          <li><%=link_to 'Cerrar sesión', :controller=>"account", :action => "logout"%></li>
                          <%else%>
                          <li><%=link_to 'Iniciar sesión', :controller=>"account", :action => "login"%></li>
                          <%end%>
                        <!-- -->
        </ul>




	</div>
</div>
<!-- end header -->
<!-- start page -->
