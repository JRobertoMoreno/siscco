<%= error_messages_for 'producto' %>

<!--[form:producto]-->
<table>
  <tr>
    <th>Descripción:</th>
    <th>Tipo:</th>
    <th>Intereses:</th>
    <th>Iva Intereses:</th>
  </tr>
  <tr>
    <td><%= text_field 'producto', 'producto'  %></td>
    <td><%= text_field 'producto', 'tipo'  %></td>
    <td><%= text_field 'producto', 'intereses'  %></td>
    <td><%= text_field 'producto', 'iva_intereses'  %></td>
  </tr>
  <tr>
    <th>Interes Moratorio:</th>
    <th>Multa:</th>
    <th>Iva Multa:</th>
    <th>Garantía:</th>
  </tr>
  <tr>
    <td><%= text_field 'producto', 'intereses_moratorio'  %></td>
    <td><%= text_field 'producto', 'multa'  %></td>
    <td><%= text_field 'producto', 'iva_multa'  %></td>
    <td><%= text_field 'producto', 'garantia'  %></td>
  </tr>
  <tr>
    <th>Periodo:</th>
    <th>Grupo:</th>    
  </tr>
  <tr>
    <td><%= collection_select('producto', 'periodo_id', $periodos, :id, :nombre, :prompt => '>>Periodo')  %></td>
    <td><%= collection_select('producto', 'grupo_id', $grupos, :id, :nombre, :prompt => '>>Grupo')  %></td>  </tr>
</table>
