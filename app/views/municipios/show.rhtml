<% for column in Municipio.content_columns %>
<p>
  <b><%= column.human_name %>:</b> <%=h @municipio.send(column.name) %>
</p>
<% end %>
<p>
  <b>Estado:</b> <%= @municipio.estado.estado %>
</p>

<%= button_to 'Editar', :action => 'edit', :id => @municipio %>
<%= link_to 'Regresar', :action => 'list' %>
