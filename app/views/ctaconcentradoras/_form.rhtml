<%= error_messages_for 'ctaconcentradora' %>

<!--[form:sector]-->
<table>
  <tr>
    <th>Num. Cuenta:</th>
    <th>Cta. Contable:</th>
    <th>Banco / Sucursal:</th>
  </tr>
  <tr>
    <td><%= text_field 'ctaconcentradora', 'num_cta',  :onkeypress => "return CharNum(event, 'numero');", :class => "text"  %></td>
    <td><%= collection_select("ctaconcentradora", "cuenta_id", @cuentas, :id, :sNombre, {:prompt => "CUENTA"}, :class => 'text') %></td>
    <td><%= collection_select("ctaconcentradora", "sucbancaria_id", @sucbancarias, :id, :descripcion, {:prompt => "BANCOS"}, :class => 'text') %></td>
  </tr>
</table>
